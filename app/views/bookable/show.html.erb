<script>

	$(document).ready(function() {
	
		$('#calendar').fullCalendar({
      
      events : [
  
      <% @bookable.events.each do |event| %>
      {
        title : '<%= "#{event[:start].strftime("%-I:%M%p")} - #{event[:end].strftime("%-I:%M%p")}" %>', 
        start : '<%= event[:start] %>',
        end : '<%= event[:end] %>'
      },
      <% end %>
      ]
			
		});
		
	});

</script>

<h2><%= @bookable.name %></h2>

<div id="feeds">
  <a href="<%= @bookable.calendar_xml %>" class="label label-xml">XML</a>
  <a href="<%= @bookable.calendar_ical %>" class="label label-ical">ICAL</a>
</div>

<div id='loading' style='display:none'>loading...</div>
<div id='calendar'></div>